<div class="jumbotron">
  <h1 class="display-4">
  <%= @contact.first_name %>
  <%= @contact.last_name %>
  </h1>
  <p class="lead">
<%= @contact.email %> |
<%= @contact.phone %> |
<%= @contact.insta %>
</p><br/>
